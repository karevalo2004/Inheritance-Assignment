public class Car extends Automobile {
    public Car(int year, String brand) {
        super(year, brand);
    }

    public void accelerate() {
        System.out.println(getBrand() + " car is accelerating.");
    }

    public void stop() {
        System.out.println(getBrand() + " car has stopped.");
    }

    public void reverse() {
        System.out.println(getBrand() + " car is reversing.");
    }

    @Override
    public void honk() {
        System.out.println(getBrand() + " car says: Honk!");
    }
}
